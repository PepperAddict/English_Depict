.dashboard-task-select-container
    .task-select
        display: flex;
        grid-gap: 10px;
        flex-wrap: wrap;
        li   
            width: 400px;
            background: white; 
            height: 200px;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.15);
            a   
                width: 100%;
                height: 100%;
                display: grid;
                justify-items: center;
                align-items: center;
